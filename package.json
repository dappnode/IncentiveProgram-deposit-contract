    "dotenv": "^8.6.0",
    "eslint": "^8.56.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.7.2",
    "hardhat": "^2.19.4",
    "hardhat-dependency-compiler": "^1.1.3",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.3.1",
    "solc-0.8": "npm:solc@^0.8.9",
