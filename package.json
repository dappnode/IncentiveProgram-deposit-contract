    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/contracts": "4.9.5",
    "@openzeppelin/contracts-upgradeable": "4.9.5",
    "@openzeppelin/hardhat-upgrades": "^1.28.0",
    "@openzeppelin/test-helpers": "^0.5.16",
